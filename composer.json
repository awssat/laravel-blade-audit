{ 
    "name": "awssat/laravel-blade-audit",
    "type": "library",
    "description": "Extensive information about any blade view in laravel's project.",
    "keywords": ["laravel", "blade", "audit", "analysis", "check", "view"],
    "license": "MIT",
    "homepage": "https://github.com/awssat/laravel-blade-audit",
    "authors": [
        {
            "name": "Awssat developers",
            "email": "hello@awssat.com"
        }
    ],
    "require": {
        "php": "~7.1",
        "illuminate/console": "~5.5|^6.0|^7.0",
        "illuminate/view": "~5.5|^6.0|^7.0",
        "illuminate/filesystem": "~5.5|^6.0|^7.0",
        "illuminate/support": "~5.5|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5"
    },
    "autoload": {
        "psr-4": {
            "Awssat\\BladeAudit\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awssat\\BladeAudit\\BladeAuditServiceProvider"
            ]
        }
    }
}
